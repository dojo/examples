{
  "name": "dojo2-widget-showcase",
  "version": "1.0.0",
  "main": "src/main.ts",
  "scripts": {
    "build": "dojo build",
    "build:ghpages": "dojo build --dojorc .dojorc-ghpages",
    "build:dev": "dojo build -m dev",
    "build:tests": "dojo build -m unit && dojo build -m functional",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"{docs,src,tests}/**/*.{ts,tsx,md}\"",
    "test-ci": "dojo build -m dist && npm run build:tests && dojo test -a -c \"browserstack\" --usr dylanschiemann2 -k 4Q2g8YAc9qeZzB2hECnS",
    "test": "dojo build -m dev && npm run build:tests && dojo test -a"
  },
  "dependencies": {
    "@dojo/framework": "^6.0.0",
    "@dojo/themes": "^6.0.0",
    "@dojo/widgets": "^6.0.0",
    "cldrjs": "0.5.0",
    "random-name": "^0.1.2",
    "tslib": "~1.9.1"
  },
  "devDependencies": {
    "@dojo/cli": "^6.0.0",
    "@dojo/cli-build-app": "^6.0.0",
    "@dojo/cli-test-intern": "^6.0.0",
    "@types/glob": "~5.0.0",
    "@types/grunt": "~0.4.0",
    "@types/node": "^6.0.46",
    "@types/sinon": "^1.16.32",
    "sinon": "^1.17.5",
    "typescript": "~3.4.5"
  }
}
