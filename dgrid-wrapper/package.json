{
  "name": "dgrid-wrapper-app",
  "version": "1.0.0",
  "scripts": {
    "prettier": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "build": "dojo build",
    "build:tests": "dojo build --legacy -m unit && dojo build --legacy -m functional",
    "test-ci": "dojo build -m dist && npm run build:tests && dojo test -a -c \"browserstack\" --usr dylanschiemann2 -k 4Q2g8YAc9qeZzB2hECnS",
    "test": "dojo build -m dist && npm run build:tests && dojo test -c local -a"
  },
  "dependencies": {
    "@dojo/framework": "^6.0.0",
    "@dojo/interop": "^6.0.0",
    "dgrid": "1.2.1",
    "dijit": "1.13.0",
    "dojo": "1.13.0",
    "dojo-dstore": "1.1.2",
    "dojo-util": "1.13.0",
    "tslib": "1.9.1"
  },
  "devDependencies": {
    "@dojo/cli": "^6.0.0",
    "@dojo/cli-build-app": "^6.0.0",
    "@dojo/cli-test-intern": "^6.0.0",
    "@types/dojo": "^1.9.40",
    "@types/glob": "~5.0.0",
    "@types/grunt": "~0.4.0",
    "@types/node": "^6.0.46",
    "@types/sinon": "^1.16.35",
    "intern": "4.3.1",
    "prettier": "1.13.7",
    "typescript": "3.4.5"
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 4,
    "useTabs": true,
    "parser": "typescript",
    "printWidth": 120,
    "arrowParens": "always"
  }
}
