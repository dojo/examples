{
  "name": "dojo2-custom-element-showcase",
  "version": "1.0.0",
  "dependencies": {
    "@dojo/core": "~0.3.0",
    "@dojo/has": "~0.1.1",
    "@dojo/i18n": "~0.3.0",
    "@dojo/shim": "~0.2.3",
    "@dojo/widget-core": "~0.4.0",
    "@dojo/widgets": "~0.3.0"
  },
  "scripts": {
    "test": "cp -r dist/accordionpane _build/ && cp -r dist/calendar _build/ && cp -r dist/combobox _build/ && cp -r dist/dialog _build/ && cp -r dist/slidepane _build/ && cp -r dist/splitpane _build/ && cp -r dist/tabpane _build/ && cp -r dist/tab _build/ && cp -r dist/titlepane _built/ && cp -r dist/tooltip _build/ && dojo test -a",
    "build": "dojo build && npm run accordionpane && npm run calendar && npm run combobox && npm run dialog && npm run slidepane && npm run splitpane && npm run tabpane && npm run tab && npm run titlepane && npm run tooltip",
    "accordionpane": "dojo build --elementPrefix=accordion-pane --element=node_modules/@dojo/widgets/accordionpane/createAccordionPaneElement",
    "calendar": "dojo build --elementPrefix=calendar --element=node_modules/@dojo/widgets/calendar/createCalendarElement",
    "combobox": "dojo build --elementPrefix=combo-box --element=node_modules/@dojo/widgets/combobox/createComboBoxElement",
    "dialog": "dojo build --elementPrefix=dialog --element=node_modules/@dojo/widgets/dialog/createDialogElement",
    "slidepane": "dojo build --elementPrefix=slide-pane --element=node_modules/@dojo/widgets/slidepane/createSlidePaneElement",
    "splitpane": "dojo build --elementPrefix=split-pane --element=node_modules/@dojo/widgets/splitpane/createSplitPaneElement",
    "tabpane": "dojo build --elementPrefix=tab-pane --element=node_modules/@dojo/widgets/tabcontroller/createTabControllerElement",
    "tab": "dojo build --elementPrefix=tab --element=node_modules/@dojo/widgets/tabcontroller/createTabElement",
    "titlepane": "dojo build --elementPrefix=title-pane --element=node_modules/@dojo/widgets/titlepane/createTitlePaneElement",
    "tooltip": "dojo build --elementPrefix=tooltip --element=node_modules/@dojo/widgets/tooltip/createTooltipElement"
  },
  "devDependencies": {
    "@dojo/cli-build-webpack": "~0.3.0",
    "@dojo/cli-test-intern": "~0.3.0",
    "@dojo/interfaces": "~0.2.0",
    "@dojo/loader": "~0.1.1",
    "@dojo/test-extras": "~0.3.0",
    "@types/chai": "~3.4.0",
    "@types/glob": "~5.0.0",
    "@types/grunt": "~0.4.0",
    "@types/node": "^6.0.46",
    "intern": "4.1.0",
    "typescript": "~2.6.1",
    "@types/sinon": "^1.16.36",
    "sinon": "^2.4.1"
  }
}
