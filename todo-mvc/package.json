{
  "name": "dojo-todo-mvc",
  "version": "0.0.1",
  "description": "TodoMVC using Dojo",
  "main": "src/main.ts",
  "scripts": {
    "build": "dojo build",
    "build:tests": "dojo build -m unit && dojo build -m functional",
    "precommit": "lint-staged",
    "prettier": "prettier --write \"{docs,src,tests}/**/*.{ts,tsx,md}\"",
    "test-ci": "dojo build -m dist && npm run build:tests && dojo test -a -c \"browserstack\" --usr dylanschiemann2 -k 4Q2g8YAc9qeZzB2hECnS",
    "test": "dojo build -m dist && npm run build:tests && dojo test -c local -a"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dojo/examples.git"
  },
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=6",
    "npm": ">=3"
  },
  "dependencies": {
    "@dojo/framework": "6.0.0-alpha.9",
    "tslib": "~1.9.1"
  },
  "devDependencies": {
    "@dojo/cli-build-app": "6.0.0-alpha.5",
    "@dojo/cli-test-intern": "./dojo-cli-test-intern-6.0.0-alpha.1.tgz",
    "@types/glob": "~5.0.0",
    "@types/node": "^6.0.46",
    "husky": "0.14.3",
    "lint-staged": "6.0.0",
    "prettier": "1.18.2",
    "typescript": "~3.4.5"
  },
  "lint-staged": {
    "*.{ts,tsx,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "tabWidth": 4,
    "useTabs": true,
    "printWidth": 120,
    "arrowParens": "always"
  }
}
