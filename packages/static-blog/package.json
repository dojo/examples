{
  "name": "static-blog",
  "version": "1.0.0",
  "scripts": {
    "dev": "dojo build --mode dev --watch --serve",
    "build": "dojo build",
    "build:dev": "dojo build --mode dev",
    "build:deploy": "npm install && dojo build --mode dist --dojorc .dojorc-deploy && shx rm -rf node_modules",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "package": "shx cp -r output/dist/ ../../output/dist/static-blog/",
    "test:ci": "echo no tests"
  },
  "main": "src/main.tsx",
  "dependencies": {
    "@dojo/framework": "~7.0.0",
    "@mapbox/rehype-prism": "0.4.0",
    "canonical-path": "1.0.0",
    "cldrjs": "0.5.1",
    "fs-extra": "9.0.0",
    "glob": "7.1.6",
    "hast-to-hyperscript": "8.0.0",
    "rehype-slug": "3.0.0",
    "remark-external-links": "6.0.0",
    "remark-frontmatter": "1.3.3",
    "remark-parse": "8.0.0",
    "remark-parse-yaml": "0.0.3",
    "remark-rehype": "6.0.0",
    "typescript": "~3.4.5",
    "unified": "9.0.0",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@dojo/cli": "~7.0.0",
    "@dojo/cli-build-app": "~7.0.0",
    "@types/fs-extra": "8.1.0",
    "@types/glob": "7.1.1",
    "@types/node": "^12.12.32",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2"
  },
  "keywords": [
    "blog",
    "blocks",
    "@dojo/framework",
    "dojo",
    "btr"
  ],
  "description": "Template for creating a blog using dojo with build time rendering and blocks"
}
